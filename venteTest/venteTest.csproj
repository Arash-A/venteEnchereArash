    <UserSecretsId>aspnet-venteTest-40D06320-FA81-4AFB-9941-66305BDF9639</UserSecretsId>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.All" Version="2.0.6" />
